local tab = DrRayLibrary.newTab("Universal", "ImageIdHere")

tab.newButton("infiniteyield", "Admin Commands", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
